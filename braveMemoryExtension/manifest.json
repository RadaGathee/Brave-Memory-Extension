{
  "manifest_version": 2,
  "name": "Brave Memory Manager",
  "version": "1.0",
  "description": "A memory management extension for Brave browser.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["brave_memory_manager.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
